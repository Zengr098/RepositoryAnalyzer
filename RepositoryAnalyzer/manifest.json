{
  "name": "Repository Analyzer",
  "id": "repositoryanalyzer",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "Repository Analyzer is an extension that performs an analysis of the contribution made by each individual developer on any public Github repository",
  "icons": {
    "16": "src/browser_action/assets/img/icon16.png",
    "48": "src/browser_action/assets/img/icon48.png",
    "128": "src/browser_action/assets/img/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "src/bg/background.html"
  },
  "action": {
    "default_icon": "src/browser_action/assets/img/icon16.png",
    "default_title": "Quantificazione del contributo",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
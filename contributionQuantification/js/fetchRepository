//Funzione che prende in input il proprietario della repository e la repository stessa producendo come risposta un JSON
function fetchRepository(ownerName, repositoryName){
  var requestURL = "https://api.github.com/repos/" + ownerName + "/"+repositoryName;
  var request = $.get(requestURL, function () {}).done(function () {
    request = request.responseJSON;
    //In request c'Ã¨ la risposta alla fetch della repository

    //link per informazioni sui collaboratori
    var contributors_url = request['contributors_url'];

    fetchDati("https://mdbootstrap.com/img/Photos/Avatars/img%20(23).jpg","Mattia","Martina","Elpidio","Alex");

  });
}

//Funzione che dai contribuitori prende tutti i dati che voglio mostrare nella nuova pagina quando cliccoSottometti
function fetchDati(image, commit, InsertLine, removeLine, graphic){

    //Trasformo l'HTML in stringhe in modo da generarlo dinamicamente
    var container = document.getElementById("containerCollaboratori");
    var containerHtml = "<div id=\"preview\" class=\"preview\">"
    containerHtml += "<div style=\"display: none;\"></div><div>"				
    containerHtml += "<div data-draggable=\"true\" style=\"position: relative;\" draggable=\"false\"><!----><!---->"
    containerHtml += "<section draggable=\"false\" class=\"container pt-5\">"
    containerHtml += "<section class=\"mb-10\">"
    containerHtml += "<div class=\"row\">"
    containerHtml += "<div class=\"col-3\">"
    containerHtml += "<img src=\""+image+" + class=\"img-fluid shadow-1-strong rounded-5\" aria-controls=\"#picker-editor\" draggable=\"false\">"
    containerHtml += "</div> <div class=\"col-9\">"
    containerHtml += "<h5 class=\"mb-3\"><strong>Anna Maria Doe</strong></h5>"
    containerHtml += "<div class=\"mb-3\"> <a class=\"text-reset\" draggable=\"false\"><i class=\"fab fa-facebook-f me-1\" aria-controls=\"#picker-editor\"></i></a>"
    containerHtml += "<a class=\"text-reset\" draggable=\"false\"><i class=\"fab fa-twitter me-1\" aria-controls=\"#picker-editor\"></i></a>"
    containerHtml += "<a class=\"text-reset\" draggable=\"false\"><i class=\"fab fa-linkedin me-1\" aria-controls=\"#picker-editor\"></i></a>"
    containerHtml += "</div> <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Distinctio est ab iure inventore dolorum consectetur? Molestiae aperiam atque quasi consequatur aut? Repellendus alias dolor ad nam, soluta distinctio quis accusantium!</p>"
    containerHtml += "</div> </div> </section></section><!----> </div> </div> </div>"

    container.innerHTML = container.innerHTML + containerHtml;

}

//Funzione che permette di ottenere dall'URL l'ownerName e la repositoryName
function extractNameRepo(){
    var url = $('#textName').val();
    alert(url)
    const myArray = url.split("/");

    fetchRepository(myArray[3], myArray[4]);

}

